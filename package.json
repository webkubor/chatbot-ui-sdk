{
  "name": "chatbot-ui-sdk",
  "version": "1.0.3",
  "type": "module",
  "description": "A universal, customizable, and lightweight chatbot frontend UI SDK.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webkubor/chatbot-ui-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/webkubor/chatbot-ui-sdk/issues"
  },
  "homepage": "https://webkubor.github.io/chatbot-ui-sdk/",
  "main": "dist/chatbot-ui-sdk.cjs.js",
  "module": "dist/chatbot-ui-sdk.es.js",
  "types": "dist/index.d.ts",
  "browser": "dist/chatbot-ui-sdk.umd.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cp index.html dist/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build",
    "publish:public": "npm publish --access public",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "chatbot",
    "ui",
    "sdk",
    "ai",
    "frontend",
    "widget"
  ],
  "author": "webkubor",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.46.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitepress": "^1.6.4",
    "vue": "^3.5.27"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
